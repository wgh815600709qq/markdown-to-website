{
    "restartable": "rs",
    "ignore": [
        ".git",
        ".svn",
        "node_modules/",
        "dist/",
        "build/",
        "model/"
    ],
    "verbose": true,
    "execMap": {
        "js": "node --harmony"
    },
    "watch": [
        "resource/"
    ],
    "env": {
        "NODE_ENV": "development"
    },
    "ext": "js json"
}